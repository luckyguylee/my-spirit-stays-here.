the_count=[1,2,3,4,5]          
fruits=['apples','oranges','pears','apricots']
change=[1,'pennies',2,'dimes',3,'quarters']      
#建立list
for number in the_count:
	print "this is count %d"%number      #依次打印the_count
	
for fruit in fruits:
	print "a fruit of types:%s"%fruit
	
for i in change:
	print "i got %r"%i
	
elements=[]   #建立空list

for i in range(0,6):
	print "adding %d to the list."%i
	elements.append(i)
	
for i in elements:
    print "element was:%d"%i
